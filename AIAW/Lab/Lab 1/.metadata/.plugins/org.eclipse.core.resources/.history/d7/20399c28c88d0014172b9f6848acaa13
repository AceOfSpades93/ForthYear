package eightPuzzle;

import java.util.Arrays;

public class Board {
	private int N;
	private byte[] board_;
	
	private byte[] arrayFlatten(byte[][] board) {
		int k = 0;
		byte[] b = new byte[board.length * board[0].length];
		
		for (byte[] row: board) {
			for (byte elem: row) {
				b[k++] = elem;
			}
		}
	
		return b;
	}
	
