package search;

public class PuzzleSolver {
	PuzzleBoard initial;
	List<PuzzleBoard> solution = new ArrayList<>();
	
	public PuzzleSolver(PuzzleBoard initial) {
		if (null == initial) {
			throw new IllegalArgumentException("Null puzzleboard presented");
		}
		
		this.initial = initial.copy();
		
	}
}
